{
    "manifest_version": 2,
    "name": "Wolverine Pack",
    "description": "See and organize your bookmarks as trees",
    "version": "1.0",
    "chrome_url_overrides" : {
    	"newtab": "/public/index.html"
    },

    // "content_scripts": [ {
    //     "css": ["/public/assets/css/normalize.css"]

    //   } ],


    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
    	"bookmarks",
        "tabs",
        "unlimitedStorage",
        "management"
    ]
}